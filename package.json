  "dependencies": {
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "netlify-lambda": "^2.0.15",
    "serverless-http": "^3.1.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}