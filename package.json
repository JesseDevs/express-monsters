{
  "name": "express-app-2",
  "version": "1.0.0",
  "description": "This is a runthrough of express practice so far.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "./node_modules/.bin/netlify-lambda serve src",
    "build": "./node_modules/.bin/netlify-lambda build src",
